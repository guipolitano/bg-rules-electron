{
  "name": "bg-rules-electron",
  "version": "0.1.0",
  "main": "src/electron.js",
  "homepage": "./",
  "private": true,
  "build": {
    "appId": "guipolitano.bg-rules-electron.app",
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.2.2",
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dist": "electron-builder",
    "build-electron": "react-scripts build && cp ./src/electron.js ./build",
    "package": "electron-builder build --win"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^22.6.0"
  }
}
